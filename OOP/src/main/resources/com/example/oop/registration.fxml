<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oop.Registracia">
   <children>
      <Button fx:id="newregister" layoutX="149.0" layoutY="312.0" mnemonicParsing="false" text="Zaregistrovať">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <AnchorPane prefHeight="78.0" prefWidth="610.0" style="-fx-background-color: #3941c6;">
         <children>
            <Label layoutX="84.0" layoutY="17.0" prefHeight="43.0" prefWidth="442.0" text="Vitajte, prosím vyplňte následujúce údaje" textFill="#a7dffc">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <TextField fx:id="name" layoutX="149.0" layoutY="114.0" prefHeight="26.0" prefWidth="351.0" promptText="Meno" />
      <TextField fx:id="surname" layoutX="148.0" layoutY="153.0" prefHeight="26.0" prefWidth="351.0" promptText="Priezvysko" />
      <TextField fx:id="loginNN" layoutX="149.0" layoutY="190.0" prefHeight="26.0" prefWidth="351.0" promptText="Používateľské meno" />
      <TextField fx:id="passwordNN" layoutX="148.0" layoutY="229.0" prefHeight="26.0" prefWidth="351.0" promptText="Heslo" />
      <TextField fx:id="passwordcorrect" layoutX="148.0" layoutY="266.0" prefHeight="26.0" prefWidth="351.0" promptText="Potvrdenie hesla" />
      <Button fx:id="back" layoutX="345.0" layoutY="312.0" mnemonicParsing="false" onAction="#goback" prefHeight="46.0" prefWidth="154.0" text="Späť">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
